# FinOps Agent Frontend Configuration Template
# Copy this file to .env.local and fill in your values

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCOUNT_ID=your-aws-account-id-here

# API Endpoints
REACT_APP_API_GATEWAY_ENDPOINT=https://your-api-gateway-id.execute-api.us-east-1.amazonaws.com/prod
REACT_APP_WEBSOCKET_API_URL=wss://your-websocket-api-id.execute-api.us-east-1.amazonaws.com/prod
REACT_APP_LEGACY_API_ENDPOINT=https://your-legacy-api-id.execute-api.us-east-1.amazonaws.com/prod/query

# Cognito Configuration
REACT_APP_COGNITO_USER_POOL_ID=your-user-pool-id
REACT_APP_COGNITO_APP_CLIENT_ID=your-app-client-id
REACT_APP_COGNITO_IDENTITY_POOL_ID=your-identity-pool-id

# Optional: Custom Configuration
REACT_APP_APP_NAME=FinOps Agent
REACT_APP_VERSION=1.0.0
